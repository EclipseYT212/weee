local time2 = 49 -- How long you want to stay in the vault, 20 is good for 1k, and makes it impossible for anyone to capture you, 45-50 for 3k.




--variables
local services = setmetatable({}, {__index = function(_, key) return game:GetService(key) end})
local WSpace = services.Workspace
local Plyr = services.Players
local RepStore = services. ReplicatedStorage
Plyr.Name = "Players"
WSpace.Name = "Workspace"
RepStore.Name = "ReplicatedStorage"
local diditrun = false
print("Script is in effect if this isn't erroring")
local times = 0
local ind = workspace.Banks:FindFirstChild('Extra', true)
local indicator = ind.Sign.Decal
-- no more vars
while true do
	wait()
	if indicator.Transparency ~= 0 and diditrun == false then
        print('its open')
		diditrun = true
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(19.7360458, 18.1664009, 840.456848) + Vector3.new(1,0,0)
        wait(1.5)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(29.309, 3.42, 815.25189) + Vector3.new(1,0,0)
        wait(2)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(18.5183678, 5.36565, 804.49) + Vector3.new(1,0,0)
        wait(time2)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1087.7, 170.5, 2022.03) + Vector3.new(1,0,0)
        times = times + 1
        print(times)
		repeat
		wait(1)
		until indicator.Transparency == 0
		diditrun = false
	end
end
