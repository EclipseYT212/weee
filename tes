local times = 20 -- How long you want to stay in the vault, 20 is good for 1k, and makes it impossible for anyone to capture you, 45-50 for 3k.




--variables
local services = setmetatable({}, {__index = function(_, key) return game:GetService(key) end})
local WSpace = services.Workspace
local Plyr = services.Players
local RepStore = services. ReplicatedStorage
Plyr.Name = "Players"
WSpace.Name = "Workspace"
RepStore.Name = "ReplicatedStorage"
local diditrun = false
print("Script is in effect if this isn't erroring")
local times = 0
local ind = workspace.Banks:FindFirstChild('Extra', true)
local indicator = ind.Sign.Decal
-- no more vars
while true do
	wait()
	if indicator.Transparency ~= 0 and diditrun == false then
        print('its open')
		diditrun = true
     local teleport = loadstring(game:HttpGet("https://raw.githubusercontent.com/EclipseYT212/test3/main/tp"))();
					teleport(CFrame.new(19.7360458, 18.1664009, 840.456848) + Vector3.new(1,0,0)
        wait(1.5)
local Players = game:GetService("Players")
local PathfindingService = game:GetService("PathfindingService")

local player = Players.LocalPlayer
local character = player.Character
local humanoid = character:WaitForChild("Humanoid")

local path = PathfindingService:CreatePath()
local targetPosition = Vector3.new((29.309, 3.42, 815.25189) -- Replace with your desired target position

local function floatCharacter()
    local success, errorMessage = pcall(function()
        path:ComputeAsync(character.PrimaryPart.Position, targetPosition)
    end)
    
    if success then
        local waypoints = path:GetWaypoints()
        local nextWaypointIndex = 1
        
        while nextWaypointIndex <= #waypoints do
            local waypoint = waypoints[nextWaypointIndex]
            local destination = waypoint.Position + Vector3.new(0, 5, 0) -- Adjust the Y value to control the floating height
            
            humanoid:MoveTo(destination)
            
            repeat
                wait()
            until (character.PrimaryPart.Position - destination).Magnitude <= 1
            
            nextWaypointIndex = nextWaypointIndex + 1
        end
    else
        warn("Failed to compute path:", errorMessage)
    end
end

floatCharacter()
    
        wait(2)
